{
  "expo": {
    "name": "Ahadu Coin",
    "slug": "ahaduCoin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ahadu-coin.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ahadu-coin.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adwa1888.ahaduCoin"
    },
    "android": {
      "package": "com.adwa1888.ahaduCoin",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ahadu-coin.png",
        "backgroundColor": "#daa520"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ahadu-coin.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d6b5f77-ac84-40e6-9947-f428a28c625d"
      }
    }
  }
}
